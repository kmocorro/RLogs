<script>

  $('document').ready(function(){

    $('#box_no').keydown(function(e){
        if (e.keyCode == 13) { // barcode scanned!
            $('#runcard_no').focus();
            return false; // block form from being submitted yet
        }
    });

  });
  
    
    function addRow(section, initRow) {
        var newRow = initRow.clone().removeAttr('id').addClass('new').insertBefore(initRow),
            deleteRow = $('<a class="rowDelete"><i class="fa fa-close"></i></a>');
    
        newRow
          
            .append(deleteRow)
            .on('click', 'a.rowDelete', function() {
                removeRow(newRow);
            })
            .slideDown(300, function() {
                $(this)
                    .find('input').focus();
            })
            //.change(($('.new > :input').removeAttr('id').attr('id', 'newInput')))
            .keydown(function(e){
              if (e.keyCode == 13){
                $('#initRow').find('input').focus();
                return false;
              }
            })
            
            //.change(($('.new > :input').removeAttr('id').attr('id', 'newInput')))
    }
            
    function removeRow(newRow) {
        newRow
            .slideUp(200, function() {
                $(this)
                    .next('div:not(#initRow)')
                        .find('input').focus()
                        .end()
                    .end()
                    .remove();
            });
    }

    $(function () {
        var initRow = $('#initRow'),
            section = initRow.parent('section');
        
        initRow.on('focus', 'input', function() {
            addRow(section, initRow);
        });
    });


</script>
