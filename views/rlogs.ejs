                    <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                            title="Collapse">
                        <i class="fa fa-minus"></i></button>
                    <!--<button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                        <i class="fa fa-times"></i></button>-->
                    </div>
                </div>
                <!-- /.box-header -->
                
                <div class="box-body table-responsive">
                    <table id="savedlogs" class="table table-hover">
                      
                      <thead>
                        <tr>
                        <th>Start Date</th>
                        <th>End Date</th>
                        <th>Process</th>
                        <th>Activity</th>
                        <th> </th>
                        </tr>
                        
                      </thead>
                        <% for (let i=0; i < rlogs_list.length; i++) {  %>
                        <tr>
                        <td><%= rlogs_list[i].startDate %></td>
                        <td><%= rlogs_list[i].endDate %></td>
                          
                          <td><% if(rlogs_list[i].process_name.length > 0){ %>
                              <% for(let j=0; j < rlogs_list[i].process_name.length; j++) { %>
                              <span class="label label-primary"><%= rlogs_list[i].process_name[j].process %></span>
                              <% } %>
                              <% } else {%>
                              <span class="label label-primary"><%= rlogs_list[i].process_name.process %></span>
                              <% } %>
                          </td>
                         
                        <td><%= rlogs_list[i].comments %></td>
                        <td>
                          <a href="#edit" data-toggle="modal" data-target="#modal-warning<%= i %>">edit</a>
                          <a href="#delete" data-toggle="modal" data-target="#modal-danger<%= i %>">delete</a>
                        </td>
                        
                        </tr>
                        <% } %>
                    </table>
                <ul id="logs"></ul>


                </div>
                <% for(let k=0; k < rlogs_list.length; k++) { %>
                <div class="modal modal-default" id="modal-danger<%= k %>">
