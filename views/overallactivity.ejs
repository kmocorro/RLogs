<script>
  $(function () {
    //$('#example1').DataTable();
    
    $('.select2').select2();

    $('#overallLogs').DataTable({
      //'order'       : [[0, 'desc']],
      'order'       : false,
      'paging'      : true,
      'lengthChange': true,
      'searching'   : true,
      'ordering'    : false,
      'info'        : true,
      'autoWidth'   : false,
      'responseive': true,
      "columnDefs": [
    { "width": "12%", "targets": [0, 1, 2] },
    { "width": "7%", "targets": [-1, -2] }
  ]
    });

    $('#date_range').daterangepicker({
      timePicker: true, 
      timePickerIncrement: 30, 
      locale: {
        format: 'MM/DD/YYYY h:mm A'
      } 
    });

    for(let i=0; i< '<%= rlogs_list.length %>';i++){
      $('#date_range_update' + i).daterangepicker({
      timePicker: true, 
      timePickerIncrement: 30, 
      locale: {
        format: 'MM/DD/YYYY h:mm A'
      } 
      });
    }
    
  });
</script>
<script>
$('document').ready(function(){

    for(let i=0; i < '<%= rlogs_list.length %>'; i++){
      
      $('#rlogs_form_update' + i).validate({
            rules:{
                date_range_update: {
                    required: true
                },
                process_name_update: {
                    required: true
                },
                comments_update:{
                    required: true
                },
                activity_id:{
                    number: true,
                    required: true
                }
            },
            messages:{
                date_range_update: 'Enter date range.',
                process_name_update: 'Enter process name affected.',
                comments_update: 'Enter your activity.',
            },
            submitHandler: submitForm_update
        });

      function submitForm_update(){

            var data = $('#rlogs_form_update' + i).serialize();
    
            $.ajax({
                type: 'POST',
                url: '/api/update',
                data: data,
                beforeSend: function(){
                    $('#error_update' + i).fadeOut();
                    $('#btn-rlogs-update'  + i).prop('disabled', true);
                    $('btn-rlogs-update' + i).html('Post Your Activities');
                },
                success: function(response){
                    if(response.success){
                        $("#btn-rlogs-update" + i).html('Saving...');
                        $("#btn-rlogs-update" + i).prop("disabled",true);
                        $("#error_update"  + i).fadeIn(0, function(){						
                            $("#error_update"  + i).html('<div class="alert alert-success">'+response.success+' </div>');
                        });
                        $("#btn-rlogs-update").html('Post Your Activities');
                        setTimeout(' window.location.reload(true); ',500);
    
                    } else {
                        $("#error_update"  + i).fadeIn(1000, function(){
    
                            $("#error_update"  + i).html('<div class="alert alert-danger">'+response.err+' </div>'); 
                            $("#btn-rlogs-update" + i).prop("disabled",false);
                            $("#btn-rlogs-update" + i).html('Try again');
    
                        });
                       
                    }
                }
            });
        
        }
    }
        
    
});
</script>
