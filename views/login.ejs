    <script type="text/javascript" src="../js/bootstrap-datetimepicker.js"></script>

    <style>
      #logs { list-style-type: none; margin: 0; padding: 0; }
      #logs li { padding: 5px 10px; }
      #logs li:nth-child(odd) { background: #eee; }
    </style>

</head>

<body class="hold-transition login-page">
<div class="login-box">
  <div class="login-logo">
    <a href="#"><b>Fab4</b>META</a>
  </div>
  <!-- /.login-logo -->
  <div class="login-box-body">
    <p class="login-box-msg">Sign in to start your session</p>
    <br/>
    <form action="" method="POST" id="login_auth" name="login_auth">
      <div class="form-group has-feedback">
        <span class="usernameLabel">Username:</span>
        <input type="text" class="form-control form-control-login" id="username" name="username" placeholder="">
      </div>
      <div class="form-group has-feedback">
        <span class="passwordLabel">Password:</span>
        <input type="password" class="form-control form-control-login" id="password" name="password" placeholder="">
      </div>
      <br/>
      <div class="row">
        <!-- /.col -->
        <div class="col-xs-8">
          <p class="bysigning">By signing up you indicate and agree that this app is a <a href="https://en.wikipedia.org/wiki/Prototype" target="_blank">prototype</a> and will be subject to change. <!--<a href="mailto:kevin.mocorro@sunpowercorp.com?subject=Fab4Meta | Sign Up&body=Fill up the required details:%0D%0A%0D%0AUsername:%0D%0AFirstname:%0D%0ALastname:%0D%0AEmail:%0D%0ADepartment:%0D%0A%0D%0AOnce registration is completed, you'll receive email verification link to change your password.%0D%0A%0D%0AThanks,%0D%0AKevin M.">Sign Up</a>--> </p>
        </div>
        <div class="col-xs-4">
          <button type="submit" id="btn-login" class="btn btn-primary btn-block btn-flat">Sign In</button>
        </div>
        <!-- /.col -->
      </div>
    </form>
   
    
    <div id="error" style="padding-top: 10px;">
        <!-- error / success will be shown here -->
    </div>

    <br/>
   
    

  </div>
  <!-- /.login-box-body -->
</div>
<!-- /.login-box -->

<!-- jQuery 3 -->
<script src="../dist/js/jquery-ui.min.js"></script>

</body>
</html>